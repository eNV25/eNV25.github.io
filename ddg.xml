<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription
  xmlns="http://a9.com/-/spec/opensearch/1.1/"
  xmlns:moz="http://www.mozilla.org/2006/browser/search/"
>
  <InputEncoding>UTF-8</InputEncoding>
  <ShortName>DuckDuckGo</ShortName>
  <Description>DuckDuckGo Search</Description>
  <Tags>duckduckgo</Tags>
  <Url type="text/html" method="post" template="https://duckduckgo.com/">
    <param name="q" value="{searchTerms}" />
  </Url>
  <Url
    type="application/x-suggestions+json"
    template="https://duckduckgo.com/ac/?q={searchTerms}&amp;type=list"
  />
  <image height="16" width="16">https://duckduckgo.com/favicon.ico</image>
  <moz:SearchForm>https://duckduckgo.com/</moz:SearchForm>
</OpenSearchDescription>
